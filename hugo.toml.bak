baseURL = 'https://blog.lazylazylazy.online/'
languageCode = 'en-us'
theme = "hugo-PaperMod"
title = "LazyBlog"

[markup]
  [markup.goldmark]
    [markup.goldmark.renderer]
      unsafe = true

[menu]
  [[menu.header]]
    name = "posts"
    weight = 0
    url = "posts"
  # [[menu.header]]
  #   name = "about"
  #   weight = 1
  #   url = "about"

[params]
user = "lazy"
hostname = "blog"
  [params.author]
    name = "Lazy Blog"  # Website and RSS feed author
    email = "zhizhiyongyonglazy@gmail.com"  # RSS feed email address


readingTime = true  # Show reading time on post page
published = true  # Show published data on post page

